$('#photos').html("<%= j render 'photos_list' %>")
<% if @item.photos.blank? %>
  $('#photo_check').hide()
<% end %>

<% if !@item.active && !@item.price.blank? && !@item.listing_name.blank? && !@item.photos.blank? && !@item.address.blank? %>
  $('#publish_button').attr('disabled', false);
<% else %>
  $('#publish_button').attr('disabled', true);
<% end %>